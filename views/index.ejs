<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.4/font/bootstrap-icons.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Pacifico&display=swap" rel="stylesheet">
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Prompt:wght@300&display=swap" rel="stylesheet">
<link rel="stylesheet" href="css/style.css">
</head>
<body>
  <%-include('../partials/header.ejs')%>
    <section id="banner">
      <div class="container">
        <div class="row">
          <h4><%=bannerRecord.title%></h4>
          <div class="col-md-8">
        
            <p><%=bannerRecord.desc%></p>
           <button class="btn btn-success"><a href="/banner">More Details</a></button>
          </div>
          <div class="col-md-4">
            <img src="/upload/<%=bannerRecord.img%>" alt="" class="img-fluid" id="banner-img">
          </div>
        </div>
      </div>
      <img src="media/border1.png" alt="">
    </section>

    <section id="services">
      <div class="container">
        <div class="row">
          <h2>services</h2>
        <%serviceRecord.forEach((result)=>{%>
          <div class="col-md-3">
            <img style="width: 100px;" src="/upload/<%=result.img%>" alt="" class="img-fluid" >
            <h4><%=result.name%></h4>
            <p><%=result.desc%></p>
           <a href="/servicedetail/<%=result.id%>"><button class="btn btn-success">More Details...</button></a> 
          </div>
        <%})%>
        </div>
      </div>
    </section>
    <section id="testi">
      <div class="container">
        <div class="row">
          <h2>Testinominals</h2>
          
            <%testiRecord.forEach((result)=>{%>
              <div class="col-md-3">
                <img src="/upload/<%=result.img%>" alt="" class="img-fluid">
                <p><span><i class="bi bi-quote"></i></span><%=result.quotes%><span><i class="bi bi-quote"></i></span></p>
                <h4><%=result.name%></h4>
              </div>
            <%})%>
        </div>
      </div>
    </section>

  <%-include('../partials/footer')%>
    <section id="links">
      <div class="container">
        <div class="row text-center">
          <div class="col-md-12">
            <a href="#"><img src="media/instagram-icon.png" alt=""></a>
            <a href="#"><img src="media/snapchat-icon.png" alt=""></a>
            <a href="#"><img src="media/linkedin-icon.png" alt=""></a>
            <a href="#"><img src="media/twitter-icon.png" alt=""></a>
          </div>
        </div>
      </div>
    </section>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>